<?xml version="1.0" encoding="UTF-8" ?>
<vhsxml version="1.1" xmlns="http://play.huggpunkt.org/vhsxml/1">
    <extensions>
        <extension>
            <uid>localfile</uid>
            <version>1</version>
            <enabled>false</enabled>
            <name>Local file interface</name>
            <description>Writing to and reading from files</description>
            <author email="jesper@huggpunkt.org" website="http://huggpunkt.org/">Jesper Räftegård</author>
            <implementation>dll</implementation>
            <input>
                <format>notEmpty</format>
                <transport>localfile</transport>
                <transport>textstream</transport>
                <transport>datastream</transport>
            </input>
            <output>
                <format>copy</format>
                <transport>localfile</transport>
                <transport>textstream</transport>
                <transport>datastream</transport>
            </output>
        </extension>
    </extensions>
</vhsxml>
