<?xml version="1.0" encoding="UTF-8" ?>
<huggPlay version="2">
    <extensions>
        <extension>
            <uid>localfile</uid>
            <version>1</version>
            <enabled>false</enabled>
            <name>Local file interface</name>
            <description>Writing to and reading from files</description>
            <author email="jesper@huggpunkt.org" website="http://huggpunkt.org/">Jesper Räftegård</author>
            <implementation>dll</implementation>
            <input>
                <format>any</format>
                <media>localfile</media>
                <media>textstream</media>
                <media>datastream</media>
            </input>
            <output>
                <format>copy</format>
                <media>localfile</media>
                <media>textstream</media>
                <media>datastream</media>
            </output>
        </extension>
    </extensions>
</huggPlay>
