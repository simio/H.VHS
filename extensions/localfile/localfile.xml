<?xml version="1.0" encoding="UTF-8" ?>
<vhsxml version="1.1" xmlns="http://play.huggpunkt.org/vhsxml/1">
    <extensions>
        <extension>
            <id releaseDate="2012-03-25T20:30:00">localfile</id>
            <name xml:lang="en">Local file access</name>
            <name xml:lang="sv">Lokal filåtkomst</name>
            <description xml:lang="en">Provides access to files on local drives.</description>
            <description xml:lang="sv">Ger åtkomst till filer på lokala enheter.</description>
            <authors>
                <author email="jesper@huggpunkt.org"
                        website="http://huggpunkt.org/"
                        username="jesper"
                        copyright="2012"
                        >Jesper Räftegård</author>
            </authors>
            <license href="http://www.isc.org/software/license">ISC License</license>
            <enabled>false</enabled>
            <condition>unstable</condition>
            <interfaces>
                <interface>org.Huggpunkt.VHS.Streams/1.0</interface>
            </interfaces>
            <source apiVersion="1.0" api="qtplugin" />
            <convertsFrom>
                <format>notEmpty</format>
                <transport>localfile</transport>
                <transport>fileuri</transport>
                <transport>datastream</transport>
            </convertsFrom>
            <convertsTo>
                <format>copy</format>
                <transport>localfile</transport>
                <transport>fileuri</transport>
                <transport>datastream</transport>
            </convertsTo>
            <audits>
                <audit username="jesper"
                       website="http://huggpunkt.org/"
                       date="2012-03-25T20:32:00"
                       email="jesper@huggpunkt.org"
                       >Jesper</audit>
            </audits>
        </extension>
    </extensions>
</vhsxml>
